local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/kirvZZ/SplixLib/main/source"))() -- Could Also Save It In Your Workspace And Do loadfile("Library.lua")()
local Window = Library:New({Name = "White?? | v1.01", Accent = Color3.fromRGB(255, 255, 255)})
--
local PlayerS = Window:Page({Name = "Player"})
--
local Players = game:GetService("Players")

local SpeedHack = PlayerS:Section({Name = "Speed-Hack", Side = "Left"})

local Func = {}

local Speed = false
local SpeedValue = 1.3




local function CreateSpeed()
	local V,ProtectedCreation = pcall(function()
		local InstanceValue = Instance.new("NumberValue")
		local p1
		local ReplicatedStorage = game.ReplicatedStorage
		local LocalName = Players.LocalPlayer.Name

		local NameValue = "SP_ADONIS"

		if ReplicatedStorage:FindFirstChild("CharStats") then
			p1 = ReplicatedStorage["CharStats"][LocalName]
			InstanceValue=SpeedValue
			InstanceValue.Name = NameValue
			InstanceValue.Parent = p1["Currents"]
		end
		print("Called add")
	end)
end

local function RemoveSpeed()
	local V,ProtectedCreation = pcall(function()
		local p1
		local ReplicatedStorage = game.ReplicatedStorage
		local LocalName = Players.LocalPlayer.Name

		local NameValue = "SP_ADONIS"

		if ReplicatedStorage:FindFirstChild("CharStats") then
			p1 = ReplicatedStorage["CharStats"][LocalName]
			if p1["Currents"]:FindFirstChild(NameValue) then
				p1["Currents"][NameValue]:Destroy()
			end
		end
		print("Called remove")
	end)
end

local function ChangeSpeed(Default)
	local V,ProtectedCreation = pcall(function()
		SpeedValue = Default
		if Speed then
			if game.ReplicatedStorage["CharStats"]:FindFirstChild(Players.LocalPlayer.Name) then
				if game.ReplicatedStorage["CharStats"][Players.LocalPlayer.Name]["Currents"]:FindFirstChild("SP_ADONIS") then
					game.ReplicatedStorage["CharStats"][Players.LocalPlayer.Name]["Currents"]["SP_ADONIS"].Value = SpeedValue
				end
			end
		end
	end)
end

local function AutoToggleSpeed()
	function TempCheck()
		if Speed == true and game.ReplicatedStorage:FindFirstChild("CharStats") then
			if game.ReplicatedStorage["CharStats"]:FindFirstChild(Players.LocalPlayer.Name) then
				CreateSpeed()
			end
		end
	end
	Players.LocalPlayer.CharacterAdded:Connect(TempCheck)
end

AutoToggleSpeed()
SpeedHack:Toggle({Title = "Speed",Default = false,Callback = function(Default) 
	Speed = Default
	print(Speed)
	if Speed == true then
		CreateSpeed()
	elseif Speed == false then
		RemoveSpeed()
	end
end})

SpeedHack:Slider({Name = "Speed-Modifier", Minimum = 0, Maximum = 10, Default = SpeedValue, Decimals = 0.1, Pointer = 0,Callback = function(Default)
	ChangeSpeed(Default)
end})

Window:Initialize()
